# Test environment configuration - prevents API calls during testing
ENVIRONMENT=test
LOG_LEVEL=WARNING

# Mock API configuration to prevent real API calls
GOOGLE_API_KEY=test_key_mock
GEMINI_MODEL_NAME=gemini-pro-test
GEMINI_TEMPERATURE=0.1
GEMINI_MAX_TOKENS=100  # Reduced for testing
GEMINI_TOP_P=0.9
GEMINI_TOP_K=40

# Test database configuration
DATABASE_URL=postgresql://react_user:react_password@localhost:5432/react_agent_test
POSTGRES_USER=react_user
POSTGRES_PASSWORD=react_password
POSTGRES_DB=react_agent_test
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# BigTool configuration for testing
BIGTOOL_ENABLED=false  # Disable during tests to avoid API calls
BIGTOOL_MAX_TOOLS=5
BIGTOOL_SIMILARITY_THRESHOLD=0.8
BIGTOOL_SEARCH_LIMIT=3

# LangGraph configuration for testing
LANGGRAPH_CHECKPOINTER_TYPE=memory  # Use memory for tests instead of PostgreSQL
LANGGRAPH_MAX_CONCURRENT_SESSIONS=2
AGENT_MAX_ITERATIONS=3
MAX_CONVERSATION_TURNS=10

# Disable external services during testing
REDIS_ENABLED=false
LOGGING_EXTERNAL=false
